{
  "suggest.timeout": 4000,
  "tsserver.npm": "/home/anders/.nvm/versions/node/v14.13.0/bin/npm",
  "python.jediEnabled": false,
  "eslint.filetypes": ["vue", "js"],
  "tabnine.priority": 90,
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    },

    "ccls": {
      "command": "/usr/bin/ccls",
      "filetypes": ["c", "cpp", "objc", "objcpp"],
      "rootPatterns": [".ccls", "compile_commands.json", ".vim/", ".git/", ".hg/"],
      "initializationOptions": {
        "cacheDirectory": "/tmp/ccls"
      }
    }
  },
  "grammarly.diagnostics": {
    "[markdown]": {
      "ignore": ["inlineCode", "code"]
    }
  },
  "explorer.position": "left"
}
